{
  "name": "virtualgit",
  "version": "1.0.0",
  "description": "In-memory Git",
  "keywords": [
    "git"
  ],
  "author": "Roger Qiu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-virtualgit.git"
  },
  "main": "dist/VirtualGit.cjs.js",
  "module": "dist/VirtualGit.es.js",
  "scripts": {
    "test": "ava -v",
    "doc": "documentation build ./lib/** -f html -o ./doc",
    "rollup": "rollup --config",
